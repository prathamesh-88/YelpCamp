            </div>

                <ul class="nav navbar-nav navbar-right navbar-options">
                <% if(!currentUser){ %>
                    <li class="list-element"><a href="/login">Login | </a></li>
                    <li class="list-element"><a href="/register">Sign in</a></li>
                <% }else{ %>
                    <li class="list-element"><a>Currently signed in as: <%= currentUser.username %> | </a></li>
                    <li class="list-element"><a href="/logout">Logout</a></li>
                <% } %>
                </ul>

        </div>
    </div>
    <div class="container flash">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert"> <%= error %> </div>
        <% } else if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert"> <%= success %> </div>
